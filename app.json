{
  "expo": {
    "name": "Painel Vitae Center",
    "slug": "painel-vitae-center",
    "version": "1.0.0",
    "orientation": "landscape",
    "platforms": ["android", "web"],
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vitaecenter.painelsenhas"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#8B4049"
      },
      "package": "com.vitaecenter.painelsenhas",
      "versionCode": 1,
      "permissions": ["INTERNET", "ACCESS_NETWORK_STATE"],
      "usesCleartextTraffic": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "210bf97f-45ea-4f94-9eeb-c7014589f92f"
      }
    },
    "owner": "giovaneluiz",
    "plugins": ["expo-video"]
  }
}
